{
  "name": "adbs",
  "version": "2.0.0",
  "description": "Seamless work with adb for multiple android devices.",
  "scripts": {
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "cleanDist": "rm dist/ -rf",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "build": "yarn cleanDist && tsc"
  },
  "engines": {
    "node": ">=6.14.4"
  },
  "bin": {
    "adbs": "dist/cli.js"
  },
  "types": "index.d.ts",
  "author": "Krzysztof 'Krizzu' Borowy",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.0",
    "execa": "^0.8.0",
    "inquirer": "^6.2.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "eslint": "^5.4.0",
    "eslint-config-krizzu": "^1.0.0",
    "jest": "23.6.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
